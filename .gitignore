# Visual Studio 2015 user specific files
GAME/.vs/
.vs/
TEST/.vs/
# Compiled Object files
*.slo
*.lo
*.o
*.obj

# Precompiled Headers
*.gch
*.pch

# Compiled Dynamic libraries
*.so
*.dylib
*.dll

# Fortran module files
*.mod

# Compiled Static libraries
*.lai
*.la
*.a
*.lib

# Executables
*.exe
*.out
*.app
*.ipa

# These project files can be generated by the engine
*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
GAME/SourceArt/**/*.png
GAME/SourceArt/**/*.tga
TEST/SourceArt/**/*.png
TEST/SourceArt/**/*.tga

# Binary Files
GAME/Binaries/*
GAME/Plugins/**/Binaries/*
TEST/Binaries/*
TEST/Plugins/**/Binaries/*

# Builds
GAME/Build/*
TEST/Build/*
# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!GAME/Build/*/
GAME/Build/*/**
!GAME/Build/*/PakBlacklist*.txt
!TEST/Build/*/
TEST/Build/*/**
!TEST/Build/*/PakBlacklist*.txt
# Don't ignore icon files in Build
!GAME/Build/**/*.ico
!TEST/Build/**/*.ico
# Built data for maps
*_BuiltData.uasset

# Configuration files generated by the Editor
GAME/Saved/*
TEST/Saved/*

# Compiled source files for the engine to use
GAME/Intermediate/*
GAME/Plugins/**/Intermediate/*
TEST/Intermediate/*
TEST/Plugins/**/Intermediate/*
# Cache files for the editor to use
GAME/DerivedDataCache/*
TEST/DerivedDataCache/*